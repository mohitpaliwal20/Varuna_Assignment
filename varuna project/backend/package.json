{
    "name": "fueleu-backend",
    "version": "1.0.0",
    "description": "Fuel EU Maritime compliance platform backend API",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "migrate": "node -r ts-node/register src/infrastructure/db/migrate.ts",
        "seed": "node -r ts-node/register src/infrastructure/db/seed.ts",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "fueleu",
        "maritime",
        "compliance"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "pg": "^8.11.3",
        "dotenv": "^16.3.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/pg": "^8.10.9",
        "@types/node": "^20.10.5",
        "@types/jest": "^29.5.11",
        "@types/supertest": "^6.0.2",
        "typescript": "^5.3.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "supertest": "^6.3.3",
        "eslint": "^8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "prettier": "^3.1.1"
    }
}